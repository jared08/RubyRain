<div class='row'>
  <div class="col-sm-1"></div>
  <div class="col-sm-8"> 
    <%= link_to "New Post", new_post_url %>

    <div id="feed">
      <%= render @posts %>
    </div>
    <div id="infinite-scrolling">
      <%= will_paginate @posts %>
    </div>

  </div>
</div>

<!-- rendering partials for modals-->
<div id="stockModal">
 <%= render 'shared/stock_modal' %>
</div>
<div id="userModal">
 <%= render 'shared/user_modal' %>
</div>
