<% provide(:title, 'New Post') %>
<h1>Add Stock</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@post) do |f| %>
      <%= f.label :title, "Title" %>
      <%= f.text_field :title %>

      <%= f.label :content, "Content" %>
      <%= f.text_field :content %>

      <%= f.submit "Add post", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
