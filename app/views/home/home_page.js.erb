$('#home_feed').append('<%= j render @tournaments %>');
$('#home_feed').append('<%= j render partial: 'news', locals: { news: @news } %>');
<% if @tournaments.next_page %>
  $('.pagination').replaceWith('<%= j will_paginate @tournaments %>');
<% else %>
  $(window).off('scroll');
  $('.pagination').remove();
<% end %>
