<% provide(:title, @stock.name) %>
<div id="stock_header">
  <%= render partial: 'stock_header', locals: { stock: @stock, time: @time, current_user: @current_user } %>
</div>

<br>

<div class="row">
  <div class="col-sm-3">
    <img src="<%= @stock.player_info[:PhotoUrl] %>" alt="@stock.name" /> <br>
    <div id="stock_earnings">
      <%= render partial: 'stock_earnings', locals: { golfer: @golfer, stock: @stock } %>
    </div>
  </div>
  <div class="col-sm-6">
    <div id="stock_earnings">
      <%= render partial: 'stock_summary', locals: { stock: @stock } %>
    </div>
    <div id="stock_graph">
      <%= render partial: 'stock_graph', locals: { stock: @stock } %>
    </div>
  </div>
  <div class="col-sm-3">
    <div id="stock_events">
      <%= render partial: 'stock_events', locals: { stock: @stock, golfer: @golfer, four_tournaments: @four_tournaments } %>
    </div>
    <div id="stock_news">
      <%= render partial: 'stock_news', locals: { stock: @stock } %>
    </div>
    <div id="stock_posts">
      <%= render partial: 'stock_posts', locals: { stock: @stock } %>
    </div>
  </div>
</div>

